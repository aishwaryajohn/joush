import matplotlib.pyplot as plt
 
# frequencies
ages = [2,9,70,40,31,45,54,46,43,40,44,
        60,7,13,57,19,90,79,32,21,30,47]
 
# setting the ranges and no. of intervals
range = (0, 100)
bins = 10 
 
# plotting a histogram
plt.hist(ages, bins, range, color = 'gold',
        histtype = 'bar', rwidth = 0.8)
 
# x-axis label
plt.xlabel('age')
# frequency label
plt.ylabel('No. of people')
# plot title
plt.title('python')
 
# function to show the plot
plt.show()


